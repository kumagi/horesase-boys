{
    "manifest_version": 2,
    "name": "Misawa-Wild",
    "description": "fastest delivery of misawa",
    "version": "0.1",
    "background": {
        "persistent":false,
        "page":"misawa.html"
    },
    "permissions": [
		    "tabs","*://*/*",
        "clipboardWrite"
    ],
    "content_scripts": [{
        "matches": ["*://*/*"],
        "js": ["jquery-1.9.1.min.js",
               "misawa-search.js"],
        "run_at": "document_end"
    }]
}
